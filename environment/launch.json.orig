{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Custom Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${command:python.interpreterPath}",
            "args": [
                "${input:PythonFile}"
            ],
            "stopAtEntry": false,
            "cwd": "${input:WorkingPath}",
            "environment": [
                {
                    "name": "OMP_NUM_THREADS",
                    "value": "1"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) cpp Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${command:python.interpreterPath}",
            "args": [
                "${workspaceFolder}/.vscode/cpp_test.py",
                "${input:Application}",
                "${input:TestNamePattern}"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "OMP_NUM_THREADS",
                    "value": "1"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) Cpp Launch (Latest)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/test/${input:TestSuiteName}",
            "args": [
                "--gtest_filter=\"${input:TestNamePattern}\""
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "OMP_NUM_THREADS",
                    "value": "1"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ],
    "inputs": [
        {
            "id": "WorkingPath",
            "type": "command",
            "command": "memento.promptString",
            "args": {
                "id": "WorkingPath",
                "description": "Working path to be used in debugging",
                "default": ""
            }
        },
        {
            "id": "PythonFile",
            "type": "command",
            "command": "memento.promptString",
            "args": {
                "id": "PythonFile",
                "description": "Python file to be executed in the working path for debugging.",
                "default": ""
            }
        },
        {
            "id": "Application",
            "type": "command",
            "command": "memento.pickString",
            "args": {
                "id": "Application",
                "description": "Application to be used",
                "options": [
                    "FluidDynamicsApplication",
                    "HDF5Application",
                    "RANSApplication",
                    "None"
                ],
                "default": "FluidDynamicsApplication"
            }
        },
        {
            "id": "TestSuiteName",
            "type": "command",
            "command": "memento.promptString",
            "args": {
                "id": "TestSuiteName",
                "description": "Test suite name",
                "default": ""
            }
        },
        {
            "id": "TestNamePattern",
            "type": "command",
            "command": "memento.promptString",
            "args": {
                "id": "TestNamePattern",
                "description": "Test name pattern",
                "default": ""
            }
        }
    ]
}